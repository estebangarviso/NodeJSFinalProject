{
	"presets": [
		[
			"minify",
			{
				"mangle": false,
				"keepFnName": true,
				"keepClassName": true,
				"removeConsole": false,
				"removeDebugger": false
			}
		]
	],
	"plugins": [
		[
			"transform-inline-environment-variables",
			{
				"exclude": ["HOSTNAME", "NODE_VERSION", "TZ", "LANG"]
			}
		]
	]
}
